---
import Links from "../components/Links.astro";

interface Props {
    title: string;
}

const { title } = Astro.props;

---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<title>{title}</title>
	</head>
	<body>
		<h1>{title}</h1>
        <Links />
	</body>
</html>
